<resources xmlns:emojicon="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/download_progress_color</item>
        <item name="android:textColorPrimaryInverse">?android:attr/colorForeground</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:textColorSecondaryInverse">@color/colorPrimaryDark</item>
        <item name="android:textColorSecondary">@color/colorPrimaryDark</item>
        <item name="android:colorForeground">@color/foreground_material_light</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.slideActivityThems">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/download_progress_color</item>
        <item name="android:textColorPrimaryInverse">?android:attr/colorForeground</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:textColorSecondaryInverse">@color/colorPrimaryDark</item>
        <item name="android:textColorSecondary">@color/colorPrimaryDark</item>
        <item name="android:colorForeground" tools:ignore="PrivateResource">
            @color/foreground_material_light
        </item>
        <item name="android:progressBarStyle">@style/progressBarStyle</item>
        <item name="android:textSelectHandleLeft" tools:ignore="PrivateResource">
            @drawable/abc_text_select_handle_left_mtrl_light
        </item>
        <item name="android:textSelectHandleRight" tools:ignore="PrivateResource">
            @drawable/abc_text_select_handle_right_mtrl_light
        </item>
        <item name="android:textSelectHandle" tools:ignore="PrivateResource">
            @drawable/abc_text_select_handle_middle_mtrl_light
        </item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:textColorPrimaryInverse">@color/TextColorPrimaryInverse</item>
        <item name="android:textColorPrimary">@color/TextcolorPrimary</item>
        <item name="android:textColorSecondary">@color/TextColorSecondary</item>
        <item name="android:textColorSecondaryInverse">@color/TextColorSecondary</item>
        <item name="android:textColorHint">@color/colorPrimaryDar</item>
        <item name="colorAccent">@color/download_progress_color</item>
    </style>

    <style name="AppTheme.NoActionBarFullAppStayle" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/download_progress_color</item>
        <item name="android:textColorPrimaryInverse">?android:attr/colorForeground</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:textColorSecondaryInverse">@color/colorPrimaryDark</item>
        <item name="android:textColorSecondary">@color/colorPrimaryDark</item>
        <item name="android:colorForeground" tools:ignore="PrivateResource">
            @color/foreground_material_light
        </item>
        <item name="android:progressBarStyle">@style/progressBarStyle</item>
        <item name="android:textSelectHandleLeft" tools:ignore="PrivateResource">
            @drawable/abc_text_select_handle_left_mtrl_light
        </item>
        <item name="android:textSelectHandleRight" tools:ignore="PrivateResource">
            @drawable/abc_text_select_handle_right_mtrl_light
        </item>
        <item name="android:textSelectHandle" tools:ignore="PrivateResource">
            @drawable/abc_text_select_handle_middle_mtrl_light
        </item>
    </style>


    <style name="AppTheme.BlackThems">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:textColorPrimaryInverse">@color/photo_view</item>
        <item name="android:textColorPrimary">@color/photo_view</item>
        <item name="android:textColorSecondary">@color/TextColorSecondary</item>
        <item name="android:textColorSecondaryInverse">@color/TextColorSecondary</item>
        <item name="android:textColorHint">@color/colorPrimaryDar</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="ThemDrawerlayout" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
        <item name="android:textColorPrimaryInverse">@android:color/secondary_text_light</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:textColorSecondaryInverse">@color/colorPrimaryDark</item>
        <item name="android:textColorSecondary">@color/colorPrimaryDark</item>
        <item name="android:colorForeground">@color/foreground_material_light</item>
    </style>

    <style name="Theme.AppTheme.Light.NoActionBar.Fullscreen" parent="android:Theme.DeviceDefault.Light.NoActionBar.Fullscreen">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <!-- Base application theme. -->
    <style name="MediaEditorTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:textColorPrimaryInverse">@color/TextColorPrimaryInverse</item>
        <item name="android:textColorPrimary">@color/TextcolorPrimary</item>
        <item name="android:textColorSecondary">@color/TextColorSecondary</item>
        <item name="android:textColorSecondaryInverse">@color/TextColorSecondary</item>
        <item name="android:textColorHint">@color/colorPrimaryDar</item>
        <item name="colorAccent">@color/download_progress_color</item>
    </style>

    <style name="EditText">
        <item name="android:singleLine">true</item>
        <item name="colorControlNormal">#FFFFFF</item>
        <item name="colorControlActivated">#FFFFFF</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="SubToolBarInMainAppHome" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/tool_bar_main_color_primary</item>
        <item name="android:textColorPrimaryInverse">?attr/colorAccent</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:textColorPrimary">@color/sub_tool_bar_colorPrimaryDark</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:textColorSecondaryInverse">@color/sub_tool_bar_colorPrimaryDark</item>
        <item name="android:textColorSecondary">@color/sub_tool_bar_colorPrimaryDark</item>
        <item name="android:colorForeground">@color/foreground_material_light</item>
    </style>

    <style name="HomeToolbar.TitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">@dimen/abc_customized_toolbar_title_size</item>
    </style>

    <style name="HomeToolbar.SubTitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">@dimen/abc_customized_toolbar_sub_title_size</item>
    </style>

    <style name="Base.V7.Widget.AppCompat.Toolbar" parent="@android:style/Widget" tools:ignore="PrivateResource">
        <item name="android:paddingLeft" tools:ignore="PrivateResource">
            @dimen/abc_action_bar_default_padding_start_material
        </item>
        <item name="android:paddingRight" tools:ignore="PrivateResource">
            @dimen/abc_action_bar_default_padding_end_material
        </item>
        <item name="android:minHeight">?actionBarSize</item>
        <item name="buttonGravity">top</item>
        <item name="collapseContentDescription" tools:ignore="PrivateResource">
            @string/abc_toolbar_collapse_description
        </item>
        <item name="collapseIcon">?homeAsUpIndicator</item>
        <item name="contentInsetStart">16.0dip</item>
        <item name="contentInsetStartWithNavigation" tools:ignore="PrivateResource">
            @dimen/abc_action_bar_content_inset_with_nav
        </item>
        <item name="maxButtonHeight" tools:ignore="PrivateResource">
            @dimen/abc_action_bar_default_height_material
        </item>
        <item name="subtitleTextAppearance">
            @style/TextAppearance.Widget.AppCompat.Toolbar.Subtitle
        </item>
        <item name="titleMargin">4.0dip</item>
        <item name="titleTextAppearance">@style/TextAppearance.Widget.AppCompat.Toolbar.Title</item>
    </style>

    <style name="buttonStart">
        <item name="android:layout_width">@dimen/_120sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">
            @drawable/common_google_signin_btn_icon_light_normal_background
        </item>
        <item name="android:gravity">center</item>
        <item name="android:text">Continue</item>
        <item name="android:textSize">@dimen/_10sdp</item>
        <item name="android:textColor">#656565</item>
    </style>
    <!-- For conversation chat item-->
    <style name="floatingButoonAttachMedia">
        <item name="android:src">@drawable/ic_add_near_color_primary</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/_8sdp</item>
        <item name="android:layout_marginRight">@dimen/_1sdp</item>
    </style>

    <style name="iconImageInMessageUserFriend">
        <item name="android:layout_width">@dimen/_30sdp</item>
        <item name="android:layout_height">@dimen/_30sdp</item>
    </style>
</resources>
